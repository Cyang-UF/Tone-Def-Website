{
  "name": "tone-dev-proj",
  "version": "1.0.0",
  "description": "Tone Def Website by Tone Dev",
  "main": "server.js",
  "scripts": {
    "test": "mocha server.test.js",
    "install-all": "npm install && cd client && npm install",
    "client": "cd client && npm start",
    "server": "nodemon server/server.js",
    "dev": "\"nodemon server/server.js\" \"cd client && npm run start\"",
    "build": "cd client && npm install && npm run-script build",
    "start": "node server/server.js",
    "heroku-postbuild": "npm install && cd client && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "author": "Luke Rouleau",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.7",
    "should": "^7.1.1"
  },
  "dependencies": {
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/nodemon": "^1.19.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mocha": "^8.3.0",
    "mongoose": "^5.11.19",
    "request": "^2.67.0",
    "validation": "0.0.1"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  }
